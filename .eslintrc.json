{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-unused-vars": "warn",
		"no-console": "off",
		"quotes": ["error", "single"],
		"semi": ["error", "always"]
	}
}
